        <type>double</type>
        <definition>static double kersol</definition>
        <argsstring>(const DoubleTab &amp;M, DoubleTab &amp;b, double eps, DoubleTab *P, DoubleTab &amp;x, DoubleTab &amp;S)</argsstring>
        <name>kersol</name>
        <param>
          <type>const <ref refid="classTRUSTTab" kindref="compound">DoubleTab</ref> &amp;</type>
          <declname>M</declname>
        </param>
        <param>
          <type><ref refid="classTRUSTTab" kindref="compound">DoubleTab</ref> &amp;</type>
          <declname>b</declname>
        </param>
        <param>
          <type>double</type>
          <declname>eps</declname>
        </param>
        <param>
